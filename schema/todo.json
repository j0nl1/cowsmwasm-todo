{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "Todo",
  "type": "object",
  "required": [
    "description",
    "id",
    "status"
  ],
  "properties": {
    "description": {
      "type": "string"
    },
    "id": {
      "type": "integer",
      "format": "uint64",
      "minimum": 0.0
    },
    "status": {
      "$ref": "#/definitions/Status"
    }
  },
  "definitions": {
    "Status": {
      "type": "string",
      "enum": [
        "OPEN",
        "COMPLETED"
      ]
    }
  }
}
