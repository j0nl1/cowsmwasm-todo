{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "title": "TodosResponse",
  "type": "object",
  "required": [
    "todos"
  ],
  "properties": {
    "todos": {
      "type": "array",
      "items": {
        "type": "array",
        "items": [
          {
            "type": "integer",
            "format": "uint64",
            "minimum": 0.0
          },
          {
            "$ref": "#/definitions/Todo"
          }
        ],
        "maxItems": 2,
        "minItems": 2
      }
    }
  },
  "definitions": {
    "Status": {
      "type": "string",
      "enum": [
        "CLOSED",
        "OPEN",
        "COMPLETED"
      ]
    },
    "Todo": {
      "type": "object",
      "required": [
        "description",
        "status"
      ],
      "properties": {
        "description": {
          "type": "string"
        },
        "status": {
          "$ref": "#/definitions/Status"
        }
      }
    }
  }
}
